{
  "command": "cat",
  "description": "Concatenate files and print to standard output",
  "global_options": [
    {
      "long": "--help",
      "short": "-?",
      "description": "Display help information"
    },
    {
      "long": "--version",
      "short": "-V",
      "description": "Display version information"
    }
  ],
  "subcommands": [],
  "options": [
    {
      "long": "--number",
      "short": "-n",
      "description": "Number all output lines"
    },
    {
      "long": "--number-nonblank",
      "short": "-b",
      "description": "Number nonempty output lines, overrides -n"
    },
    {
      "long": "--squeeze-blank",
      "short": "-s",
      "description": "Squeeze multiple adjacent empty lines to one"
    },
    {
      "long": "--show-ends",
      "short": "-E",
      "description": "Display $ at end of each line"
    },
    {
      "long": "--show-tabs",
      "short": "-T",
      "description": "Display TAB characters as ^I"
    },
    {
      "long": "--show-nonprinting",
      "short": "-v",
      "description": "Use ^ and M- notation for non-printing characters"
    },
    {
      "long": "--show-all",
      "short": "-A",
      "description": "Equivalent to -vET"
    },
    {
      "short": "-e",
      "description": "Equivalent to -vE"
    },
    {
      "short": "-t",
      "description": "Equivalent to -vT"
    }
  ],
  "arguments": [
    {
      "name": "files",
      "description": "Files to concatenate (default: standard input)"
    }
  ]
}