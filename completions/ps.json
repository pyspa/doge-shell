{
  "command": "ps",
  "description": "Report a snapshot of the current processes",
  "global_options": [
    {
      "short": "-a",
      "description": "Select all processes except session leaders and processes not associated with a terminal"
    },
    {
      "short": "-A",
      "long": "--all",
      "description": "Select all processes"
    },
    {
      "short": "-e",
      "description": "Select all processes (identical to -A)"
    },
    {
      "short": "-u",
      "description": "Display user-oriented format"
    },
    {
      "short": "-x",
      "description": "Lift the BSD-style must have a tty restriction"
    },
    {
      "short": "-f",
      "description": "Do full-format listing"
    },
    {
      "short": "-l",
      "description": "Long format"
    },
    {
      "short": "-j",
      "description": "Jobs format"
    },
    {
      "short": "-o",
      "description": "User-defined format",
      "takes_value": true,
      "value_type": { "type": "String" }
    },
    {
      "long": "--sort",
      "description": "Specify sorting order",
      "takes_value": true,
      "value_type": {
        "type": "Choice",
        "data": [
          "pid",
          "ppid",
          "user",
          "uid",
          "cpu",
          "mem",
          "vsz",
          "rss",
          "tty",
          "stat",
          "start",
          "time",
          "comm",
          "args"
        ]
      }
    },
    {
      "short": "-p",
      "long": "--pid",
      "description": "Select by PID",
      "takes_value": true,
      "value_type": { "type": "Number" }
    },
    {
      "short": "-U",
      "long": "--user",
      "description": "Select by real user ID or name",
      "takes_value": true,
      "value_type": { "type": "String" }
    },
    {
      "short": "-C",
      "description": "Select by command name",
      "takes_value": true,
      "value_type": { "type": "String" }
    },
    {
      "long": "--forest",
      "description": "ASCII art process tree"
    },
    {
      "long": "--headers",
      "description": "Repeat header lines"
    },
    {
      "long": "--no-headers",
      "description": "Print no header line at all"
    },
    {
      "short": "-w",
      "description": "Wide output"
    },
    {
      "long": "--help",
      "description": "Display help information"
    },
    {
      "long": "--version",
      "short": "-V",
      "description": "Display version information"
    }
  ],
  "subcommands": []
}
