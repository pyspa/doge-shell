      "options": [
        {
          "long": "--time",
          "short": "-t",
          "description": "Seconds to wait for stop before killing"
        }
      ],
      "arguments": [
        {
          "name": "container",
          "description": "Container name or ID",
          "type": {
            "type": "Script",
            "data": "docker ps --format '{{.Names}}'"
          }
        }
      ],
      "subcommands": []
    },
    {
      "name": "rm",
      "description": "Remove one or more containers",
