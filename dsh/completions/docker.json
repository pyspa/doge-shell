      "arguments": [
        {
          "name": "container",
          "description": "Container name or ID",
          "type": {
            "type": "Script",
            "data": "docker ps -a --format '{{.Names}}' 2>/dev/null"
          }
        }
      ],
      "subcommands": []
    },
    {
