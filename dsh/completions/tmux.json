{
  "command": "tmux",
  "description": "Terminal multiplexer",
  "global_options": [
    {
      "short": "-2",
      "description": "Force tmux to assume the terminal supports 256 colours"
    },
    {
      "short": "-C",
      "description": "Start in control mode"
    },
    {
      "short": "-c",
      "description": "Execute a shell command"
    },
    {
      "short": "-f",
      "description": "Specify an alternative configuration file"
    },
    {
      "short": "-L",
      "description": "Specify the socket name"
    },
    {
      "short": "-l",
      "description": "Behave as a login shell"
    },
    {
      "short": "-S",
      "description": "Specify the socket path"
    },
    {
      "short": "-u",
      "description": "Write UTF-8 output"
    },
    {
      "short": "-v",
      "description": "Request verbose logging"
    },
    {
      "short": "-V",
      "description": "Report the tmux version"
    }
  ],
  "subcommands": [
    {
      "name": "attach-session",
      "description": "Attach to an existing session",
      "options": [
        {
          "short": "-t",
          "description": "Target session",
          "takes_value": true,
          "value_type": {
            "type": "Script",
            "data": "tmux list-sessions -F '#{session_name}' 2>/dev/null"
          }
        },
        {
          "short": "-d",
          "description": "Detach other clients"
        }
      ],
      "arguments": [
        {
          "name": "session",
          "description": "Session name",
          "type": {
            "type": "Script",
            "data": "tmux list-sessions -F '#{session_name}' 2>/dev/null"
          }
        }
      ]
    },
    {
      "name": "attach",
      "description": "Attach to an existing session (alias)",
      "options": [
        {
          "short": "-t",
          "description": "Target session",
          "takes_value": true,
          "value_type": {
            "type": "Script",
            "data": "tmux list-sessions -F '#{session_name}' 2>/dev/null"
          }
        },
        {
          "short": "-d",
          "description": "Detach other clients"
        }
      ],
      "arguments": [
        {
          "name": "session",
          "description": "Session name",
          "type": {
            "type": "Script",
            "data": "tmux list-sessions -F '#{session_name}' 2>/dev/null"
          }
        }
      ]
    },
    {
      "name": "a",
      "description": "Attach to an existing session (alias)",
      "arguments": [
        {
          "name": "session",
          "description": "Session name",
          "type": {
            "type": "Script",
            "data": "tmux list-sessions -F '#{session_name}' 2>/dev/null"
          }
        }
      ]
    },
    {
      "name": "detach-client",
      "description": "Detach the current client"
    },
    {
      "name": "has-session",
      "description": "Check if a session exists",
      "options": [
        {
          "short": "-t",
          "description": "Target session",
          "takes_value": true,
          "value_type": {
            "type": "Script",
            "data": "tmux list-sessions -F '#{session_name}' 2>/dev/null"
          }
        }
      ]
    },
    {
      "name": "kill-server",
      "description": "Kill the tmux server"
    },
    {
      "name": "kill-session",
      "description": "Kill a session",
      "options": [
        {
          "short": "-t",
          "description": "Target session",
          "takes_value": true,
          "value_type": {
            "type": "Script",
            "data": "tmux list-sessions -F '#{session_name}' 2>/dev/null"
          }
        }
      ],
      "arguments": [
        {
          "name": "session",
          "description": "Session name",
          "type": {
            "type": "Script",
            "data": "tmux list-sessions -F '#{session_name}' 2>/dev/null"
          }
        }
      ]
    },
    {
      "name": "list-clients",
      "description": "List all clients attached to the server"
    },
    {
      "name": "list-commands",
      "description": "List all tmux commands"
    },
    {
      "name": "list-keys",
      "description": "List all key bindings"
    },
    {
      "name": "list-sessions",
      "description": "List all sessions"
    },
    {
      "name": "ls",
      "description": "List all sessions (alias)"
    },
    {
      "name": "new-session",
      "description": "Create a new session",
      "options": [
        {
          "short": "-s",
          "description": "Session name"
        },
        {
          "short": "-d",
          "description": "Start session in detached mode"
        },
        {
          "short": "-n",
          "description": "Window name"
        },
        {
          "short": "-c",
          "description": "Start directory"
        }
      ]
    },
    {
      "name": "new",
      "description": "Create a new session (alias)",
      "options": [
        {
          "short": "-s",
          "description": "Session name"
        },
        {
          "short": "-d",
          "description": "Start session in detached mode"
        }
      ]
    },
    {
      "name": "rename-session",
      "description": "Rename a session",
      "options": [
        {
          "short": "-t",
          "description": "Target session",
          "takes_value": true,
          "value_type": {
            "type": "Script",
            "data": "tmux list-sessions -F '#{session_name}' 2>/dev/null"
          }
        }
      ],
      "arguments": [
        {
          "name": "new-name",
          "description": "New session name"
        }
      ]
    },
    {
      "name": "switch-client",
      "description": "Switch the current client to another session",
      "options": [
        {
          "short": "-t",
          "description": "Target session",
          "takes_value": true,
          "value_type": {
            "type": "Script",
            "data": "tmux list-sessions -F '#{session_name}' 2>/dev/null"
          }
        }
      ]
    },
    {
      "name": "source-file",
      "description": "Execute commands from a file",
      "arguments": [
        {
          "name": "file",
          "description": "Configuration file",
          "type": {
            "type": "File",
            "data": {}
          }
        }
      ]
    },
    {
      "name": "send-keys",
      "description": "Send keys to a window",
      "options": [
        {
          "short": "-t",
          "description": "Target pane",
          "takes_value": true,
          "value_type": {
            "type": "Script",
            "data": "tmux list-sessions -F '#{session_name}' 2>/dev/null"
          }
        }
      ]
    }
  ]
}
